{
  "name": "work-in-france",
  "version": "0.0.1",
  "description": "La plateforme de demande d'autorisation provisoires de travail.",
  "author": "",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwiss/work-in-france.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "main": "index.js",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "eslint --ext .jsx,.js ./pages ./components ./lib ./utils ./theme",
    "deploy": "npm run build && npm run export"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@socialgouv/react-departements": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "axios": "^0.19.2",
    "date-fns": "^2.10.0",
    "formik": "^2.1.4",
    "memoizee": "^0.4.14",
    "next": "^9.3.0",
    "next-redux-wrapper": "^5.0.0",
    "normalize.css": "^8.0.1",
    "raw-loader": "^4.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-markdown": "^4.3.1",
    "react-query": "^1.0.23",
    "react-redux": "^7.2.0",
    "react-select": "^3.0.8",
    "rebass": "3.1.0",
    "recharts": "^2.0.0-beta.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.1",
    "url-search-params-polyfill": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "flow-bin": "^0.120.1",
    "flow-typed": "^2.5.1",
    "husky": "^4.2.3",
    "prettier": "^1.16.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": "^[A-Z]+-[0-9]+ - .*",
      "subjectPatternErrorMsg": "Subject must be in format 'CMS-123 - Commit message'",
      "helpMessage": ""
    }
  }
}
